@model AdminIncomeDetail
@using BackSideWeb.Models.ViewModel
@using JxBackendService.Model.Entity
@using BackSideWeb.Model.Entity.MM
@using JxBackendService.Model.Enums
@using MS.Core.MMModel.Models.AdminIncomeExpense;

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
<link href="~/css/user.css" rel="stylesheet" />
<div class="wrapper">
    <div class="info">
        <div><span class="title">收益单ID</span> @Model.Id</div>
        <div><span class="title">帖子ID</span> @Model.PostId</div>
        <div><span class="title">帖子区域</span> @Model.CategoryText</div>
        <div><span class="title">收益会员ID</span> @Model.UserId</div>
        <div><span class="title">解锁单ID</span> @Model.SourceId</div>
