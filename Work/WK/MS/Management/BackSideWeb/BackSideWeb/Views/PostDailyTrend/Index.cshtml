@using BackSideWeb.Models.ViewModel
@{
    ViewData["Title"] = "帖子日趋势";
}
<div class="bg spacing gap system_filter" id="jqMemberFilter">
    <br />
    	<div class="col">

		<div class="item jqAdvanceFilter">
			<div class="col_p_1">
				<div class="label_input column_layout">
					<div class="form_set datepicker_text">
						@Html.TextBox("jqStartDate", DateTime.Today.AddDays(-31).ToString("yyyy-MM-dd"), new { id = "jqStartDate", @class = "width_set jqFilter" })
						<span>至</span>
						@Html.TextBox("jqEndDate", DateTime.Today.ToString("yyyy-MM-dd"), new { id = "jqEndDate", @class = "width_set jqFilter" })
					</div>
				</div>
			</div>
		</div>
	</div>

    <div class="col">
		<div class="item">
			<div class="btns">
				<a href="javascript:;" class="btn_default main_color jqSearchBtn">查询</a>
				<a href="javascript:;" class="btn_default main_color_line jqResetBtn">清空</a>
			</div>
		</div>
	</div>

</div>

<div id="jqContentRoot" style="display:none">
    <div class="bg gap">
        <div class="member_center_report agent_report table_overflow_x">
            <table>
                <thead>
                    <tr>
                        <th>日期</th>
                        <th>展示中帖子总数</th>
                        <th>广场展示中</th>
                        <th>广场审核中</th>
                        <th>广场未通过</th>
                        <th>广场被解锁次数</th>
                        <th>寻芳阁展示中</th>
                        <th>寻芳阁审核中</th>
                        <th>寻芳阁未通过</th>
                        <th>寻芳阁被解锁次数</th>
                        <th>官方展示中</th>
                        <th>官方审核中</th>
                        <th>官方未通过</th>
                        <th>官方被预约次数</th>
                        <th>官方被预约取消次数</th>
                    </tr>
                </thead>
                <tbody id="jqGridContent">
                </tbody>
            </table>
        </div>
    </div>
    <div class="footer_function_content space_between">
        <div class="jqPagination pagination"></div>
    </div>
</div>