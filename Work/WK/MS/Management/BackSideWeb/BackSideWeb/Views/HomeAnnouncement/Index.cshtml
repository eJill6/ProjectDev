@{
    var startDatedefault = DateTime.Now.ToString("yyyy-MM-dd");
    var endDatedefault = DateTime.Now.ToString("yyyy-MM-dd");
    var selectData= new List<SelectListItem>();
    selectData.Add(new SelectListItem { Text = "全部", Value = null });
    selectData.Add(new SelectListItem { Text = "显示", Value = "true" });
    selectData.Add(new SelectListItem { Text = "隐藏", Value = "false" });
}

<a href="javascript:;" class="jqSearchBtn" style="display:none">查询</a>

<div class="pageTab max_width_limited" id="pageTab">
    <ul><a id="1" href="javascript:void(0)">公告</a></ul>
    <ul><a id="2" href="javascript:void(0)">首页盖台</a></ul>
</div>
<label id="homeAnnouncementTypeTemp" hidden></label>

<input type="hidden" id="type" name="type" />
<div class="bg spacing gap system_filter" id="jqAnnouncementFilter">
    <br />

    <div class="col">
        <div class="item">
            <div class="label_input column_layout">
                <label for="">
                    <p>标题</p>
                </label>
                <div class="form_set">
                    <input class="jqFilter" type="text" name="" id="title" placeholder="" value="" maxlength="20">
                </div>
            </div>
        </div>
    </div>

    <div class="col">
        <div class="item">
            <div class="col_p_1">
                <div class="label_input column_layout">
                    <label for="" class="m_hide">
                        创建时间
                    </label>
                    <div class="form_set datepicker_text">
                        @Html.TextBox("jqStartDate", "", new { id = "jqStartDate", @class = "width_set jqFilter" })
                        <span>至</span>
                        @Html.TextBox("jqEndDate", "", new { id = "jqEndDate", @class = "width_set jqFilter" })
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="item jqAdvanceFilter">
            <div class="label_input column_layout">
                <label for="">
                    <p>显示</p>
                </label>
                @await Html.PartialAsync("Partial/_DropdownMenu", new DropdownMenuSetting
                {
                SettingId = "jqIsActiveSelectList",
                Items = selectData,
                })
            </div>
        </div>
    </div>
    <div class="col">

        <div class="item">
            <div class="btns">
                <a href="javascript:;" class="btn_default main_color jqSearchBtn">查询</a>
                <a href="javascript:;" class="btn_default main_color_line jqResetBtn">清空</a>
            </div>
        </div>
    </div>
</div>

<div id="jqContentRoot" style="display:none">
    <div class="bg gap">
        <div class="member_center_report agent_report table_overflow_x">
            <table>
                <thead>
                    <tr>
                        <th class="AnnouncementType1">更新时间</th>
                        <th class="AnnouncementType2">排序</th>
                        <th class="AnnouncementType2">标题</th>
                        <th>内文</th>
                        <th class="AnnouncementType2">创建时间</th>
                        <th class="AnnouncementType2">开始时间</th>
                        <th class="AnnouncementType2">结束时间</th>
                        <th>创建人</th>
                        <th>显示</th>
                        <th>编辑</th>
                        <th class="AnnouncementType2">删除</th>
                    </tr>
                </thead>
                <tbody id="jqGridContent">
                </tbody>
            </table>
        </div>
    </div>
    <div class="footer_function_content space_between">
        <div class="jqPagination pagination"></div>
    </div>
</div>

