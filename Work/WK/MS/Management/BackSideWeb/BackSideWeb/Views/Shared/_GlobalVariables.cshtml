@using JxBackendService.Common.Util
@using JxBackendService.Model.Enums;
@using JxBackendService.Model.MessageQueue;
@{
    var rabbitMQWebSocketSetting = ViewBag.EndUserRabbitMQWebSocketSettings as List<RabbitMQWebSocketSetting>;    
}
<script>
    window.globalVariables = {
        WebRoot: "/",
        GetUrl: function (url) {
            return this.WebRoot + url.replace(/^\//, "");
        },
        endUserRabbitMQWebSocketSettings: @Html.Raw(rabbitMQWebSocketSetting.ToJsonString(isCamelCaseNaming: true)),
        dbSyncResponseMilliSeconds: @GlobalVariables.DBSyncResponseMilliSeconds,
    };
</script>