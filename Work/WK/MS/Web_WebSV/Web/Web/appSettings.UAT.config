<?xml version="1.0" encoding="utf-8" ?>
<appSettings>
  <!--Application Name-->
  <add key="AppName" value="web" />
  <!--Rabbit MQ stomp plugin service url-->
  <add key="StompServiceUrl" value="https://ms-lottory-mq.ioiskp.com/stomp" />
  <!--ServiceUrl -->
  <!--SV服务地址-->
  <add key="ServiceUrl" value="http://localhost/SLPolyGame.Web" />
  <!--.Net Core SV服务地址-->
  <add key="CoreServiceUrl" value="http://localhost:5185" />
  
  <!--应急SV服务地址-->
  <add key="BackupServiceUrl" value="http://localhost/SLPolyGame.Web" />

  <!--公共缓存服务器地址，默认端口27777-->
  <!--<add key="memcachedAddres" value="192.168.3.13:11211" />-->
  <!--静态资源CDN配置 -->
  <add key="UseCDN" value="false" />
  <add key="CDNSite" value="https://ms-lottory-web.ioiskp.com" />

  <!--扣除的反水/GD說-就是在算奖的时候，0.001是不参与算奖的，它只参与投注的反水-->
  <!--只有0.078的用户，算奖时按照0.077来算，反水时按照0.078来反-->
  <add key="ExportRebatePro" value="" />
  <!--獎金折扣-->
  <add key="DiscountPro" value="" />

  <!--環境變數:資料種類有  Development, SIT, UAT, Production-->
  <add key="Environment" value="UAT" />
  <!--商戶代碼-->
  <add key="PlatformMerchantCode" value="MSL" />

  <!--偵錯UserIDs 多筆;相隔-->
  <add key="DebugUserIds" value="6251;888;588" />
  <!--偵錯User不開啟全屏-->
  <add key="IsDebuggerNoFullScreen" value="1" />

  <!--登入Token加解密key-->
  <add key="AuthenticationTokenKey" value="#EDC4RFV" />

  <!--MSService的Url-->
  <add key="MSServiceUrl" value="http://localhost:5172"/>
</appSettings>